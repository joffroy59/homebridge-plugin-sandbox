{
  "pluginAlias": "Test1HomebridgePlugin",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Property",
        "type": "string",
        "required": true,
        "default": "Test1 Value"
      },
      "devices": {
        "title": "Devices",
        "type": "TypedArray",
        "description": "The Devices",
        "items": {
          "title": "Device",
          "type": "object",
          "properties": {
            "name": {
              "title": "Name",
              "type": "string",
              "required": true,
              "examples": [
                "Standing Device"
              ]
            },
            "updateInterval": {
              "title": "Interval in which the state of the Accessory should be updated (in seconds)",
              "type": "number",
              "default": "10",
              "required": false
            }
          }
        }
      }
    }
  }
}